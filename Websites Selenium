package seleniumtask;
import java.util.ArrayList;
import java.util.List;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;
import io.github.bonigarcia.wdm.WebDriverManager;


public class WebsiteTask {

	public static void main(String[] args) {
		List<String> web = new ArrayList<String>();
		web.add("https://www.flipkart.com");
		web.add("https://www.amazon.in");
		web.add("https://www.youtube.com");
		web.add("https://www.google.com");
		web.add("https://www.wikipedia.org");
		
		WebDriverManager.chromedriver().setup();
	    WebDriver driver = new ChromeDriver();
	     
	    for(String webURL:web) {
	    	driver.get(webURL);
	    	System.out.println("websitetitle "+driver.getTitle());
		    System.out.println("websiteURL "+driver.getCurrentUrl());
		   
	    }
	    
	    
	    
	    
	    driver.quit();

	}

}
