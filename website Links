package seleniumtwo;
import org.openqa.selenium.chrome.ChromeDriver;
import java.util.List;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.By;

public class LocatorXpath{

	public static void main(String[] args) {
		
		WebDriver driver = new ChromeDriver();
		driver.get("https://academybugs.com/contact-us-form/");
		
        System.out.println("List of links that are presented in this website: ");
		List<WebElement>link = driver.findElements(By.xpath("//li"));
		for(WebElement linkname:link) {
			System.out.println(linkname.getText());
		}
		driver.close();
	}

}
