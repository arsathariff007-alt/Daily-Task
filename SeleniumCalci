package seleniumtwo;
import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;

public class Calci {

	public static void main(String[] args) {
		WebDriver driver = new ChromeDriver();
		driver.get("https://testsheepnz.github.io/BasicCalculator.html");
		
		WebElement one = driver.findElement(By.id("number1Field"));
		one.sendKeys("16");
		
		WebElement two = driver.findElement(By.id("number2Field"));
		two.sendKeys("16");
		
		WebElement ans = driver.findElement(By.id("numberAnswerField"));
		ans.sendKeys("16");
		
WebElement abcLink = driver.findElement(By.linkText("AUTOMATION KATA"));
		
		
		String link = abcLink.getText();
		System.out.println("Link name:"+link);
		driver.close();	
		
		
		

	}

}
